package bstreelinklistinterfgeneric;

import bstreeInterface.BinarySearchTree;
import exceptions.*;

class Node<E> {
    E data;
    Node<E> left, right;

    Node(E data) {
        this.data = data;
        this.left = this.right = null;
    }
}

public class LinkedBST<E extends Comparable<E>> implements BinarySearchTree<E> {
    private Node<E> root;

    public LinkedBST() {
        this.root = null;
    }
